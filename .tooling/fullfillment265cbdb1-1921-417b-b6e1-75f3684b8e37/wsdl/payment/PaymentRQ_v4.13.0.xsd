							<xs:element name="Rail" type="RailType" minOccurs="0" maxOccurs="5"/>
							<xs:element name="ShippingAddress" type="AddressType" minOccurs="0"/>
							<xs:element name="Remarks" type="xs:string" minOccurs="0"/>
							<xs:element name="MCP" type="MCPType" minOccurs="0"/>
						</xs:sequence>
						<xs:attribute name="UniqueBookingID" type="xs:string">
							<xs:annotation>
								<xs:documentation>Identifier for tracking transactions originating from the same booking/PNR</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="SessionID" type="xs:string">
							<xs:annotation>
								<xs:documentation>Identifier for tracking transactions from the same session</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="OrderID" type="xs:string">
							<xs:annotation>
								<xs:documentation>Identifier for tracking purposes.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="OrderType" type="xs:string">
							<xs:annotation>
								<xs:documentation>O - Original (default)
									EE - Even Exchange
									EA - Exchange w/ Add Collect
									ER - Exchange w/ Refund
									R - Refund</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="RecordLocator" type="xs:string">
							<xs:annotation>
								<xs:documentation>PNR Record locator</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="DollarAmountPaid" type="Money" use="optional"/>
						<xs:attribute name="InvoiceNumber" type="xs:string"/>
						<xs:attribute name="ThirdPartyBookingInd" type="xs:boolean">
							<xs:annotation>
								<xs:documentation>Indicates if the booking was made by a 3rd party. i.e. if the card holder is not one of the passengers, set this to true.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="OneWayInd" type="xs:boolean">
							<xs:annotation>
								<xs:documentation>Set to true if the booking is a one-way trip.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="MultiCityInd" type="xs:boolean">
							<xs:annotation>
								<xs:documentation>Set to true if the booking involves more than 1 destination cities.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="PromotionCode" type="xs:string">
							<xs:annotation>
								<xs:documentation>For BML:
