<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xsd:schema xmlns="http://services.sabre.com/STL/v01" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://services.sabre.com/STL/v01" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.4.0">
  <xsd:annotation>
    <xsd:documentation>from the STL_Headers_v.1.0.2 package with namespace http://services.sabre.com/STL/v01</xsd:documentation>
  </xsd:annotation>
  <xsd:annotation>
    <xsd:documentation>4-16-2010 dmh - added ConversationID, MessageID and Version to RQ and RS.</xsd:documentation>
  </xsd:annotation>
  <xsd:complexType name="AirportTax">
    <xsd:annotation>
      <xsd:documentation>Domestic airport taxes collected from and paid by the customer</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Amount" type="Amount" minOccurs="0"/>
    </xsd:sequence>
    <xsd:attribute name="taxCode">
      <xsd:annotation>
        <xsd:documentation>A code that applies to the tax (e.g, US, ZP)</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="AlphaNumericStringLength2to3">
          <xsd:enumeration value="XF"/>
          <xsd:enumeration value="ZP"/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="airportCode" type="CityCode" use="required">
      <xsd:annotation>
        <xsd:documentation>A city code associated to ZP and XF taxes</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="Amount">
    <xsd:simpleContent>
      <xsd:extension base="_amount">
        <xsd:attribute name="currencyCode" type="CurrencyCode" default="USD"/>
        <xsd:attribute name="currency" type="ProperName"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="Amount.Basic">
    <xsd:sequence>
      <xsd:element name="Amount" type="Amount">
        <xsd:annotation>
          <xsd:documentation>Actual amount</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Text" type="StringLength1to25" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Any text associated to the amount.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ApplyCreditInd" type="xsd:boolean" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Boolean value set to 'Y' if the amount is negative</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Calculate.MiscTransInfo">
    <xsd:sequence>
      <xsd:element name="InputCurrency" type="CurrencyCode" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Currency code inputted by the agent and used to calculate fees.  Mapped to:  SR05_11_ICUR</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ExemptGSTTax" type="xsd:boolean" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>An indicator to exempt government sales tax (GST) in Canada.  Mapped to: SR05_11_TEXP,#SR05_11_GSTP</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ExemptPSTTax" type="xsd:boolean" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>An indicator to exempt provincial sales tax (GST) in Canada.  Mapped to: SR05_11_TEXP,#SR05_11_PSTP</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="CalculateTotalCostRQ">
    <xsd:sequence>
      <xsd:element name="Header" type="STL_Header.RQ" minOccurs="0"/>
      <xsd:element name="AgentPOS" type="POS.Misc"/>
      <xsd:element name="Transaction" type="MiscTransInfo"/>
      <xsd:element name="Parameters" type="Calculate.MiscTransInfo" minOccurs="0"/>
      <xsd:element name="Fees" type="FeesType" maxOccurs="99"/>
    </xsd:sequence>
    <xsd:attributeGroup ref="MISC_PayloadStdAttributes"/>
  </xsd:complexType>
  <xsd:complexType name="CalculateTotalCostRS">
    <xsd:sequence>
      <xsd:element name="Header" type="STL_Header.RS"/>
      <xsd:element name="AgentPOS" type="POS.Misc" minOccurs="0"/>
      <xsd:element name="Transaction" type="MiscTransInfo" minOccurs="0"/>
      <xsd:element name="Parameters" type="Calculate.MiscTransInfo" minOccurs="0"/>
      <xsd:element name="Fees" type="FeesType" minOccurs="0" maxOccurs="99"/>
      <xsd:element name="TotalCost" type="Amount" minOccurs="0"/>
    </xsd:sequence>
    <xsd:attributeGroup ref="MISC_PayloadStdAttributes"/>
  </xsd:complexType>
  <xsd:complexType name="Check.Misc">
    <xsd:sequence>
      <xsd:element name="MICRNumber" type="StringLength1to25" minOccurs="0"/>
      <xsd:element name="ApprovalCode" type="AlphaNumericStringLength1to8" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Collect.MiscTransInfo">
    <xsd:sequence>
      <xsd:element name="PrinterLniata" type="StringLength6" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>A printer line address where the document will be printed.  A printer should have been assigned by the agent (e.g., F20204)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PrinterStock" type="AlphaNumericStringLength2" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>A stock code used to assign a printer.  Mapped to:  SR05_11_PSTK</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PrintTaxCpn" type="xsd:boolean" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>An indicator  that a tax coupon will be printed.  Mapped to:  SR05_11_TCPN</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CarrierOverride" type="AlphaNumericStringLength2to3" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Validating carrier override for host partitions containing more than one carrier (ie LAN). Mapped to:
            SR05_11_OVR, SR05_11_OVRE</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AgencyIdOverride" type="EmptyOrNumericStringLength7" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>AGID - Agency ID override. An override for non-IATA "direct connect" Agency 7-digit numeric identifier or a blank value (meaning that the AGID element should not be added to the resulting EMD document). In case it’s not provided, the AGID element will be copied to the EMD from the PNR. Mapped to: SR05_D3_DATA with SR05_D3_TYPE = 'A'</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="CollectMiscFeeRQ">
    <xsd:sequence>
      <xsd:element name="Header" type="STL_Header.RQ" minOccurs="0"/>
      <xsd:element name="AgentPOS" type="POS.Misc"/>
      <xsd:element name="Transaction" type="MiscTransInfo"/>
      <xsd:element name="Parameters" type="Collect.MiscTransInfo" minOccurs="0"/>
      <xsd:element name="Fees" type="FeesType" maxOccurs="99"/>
      <xsd:element name="TotalCost" type="Amount">
        <xsd:annotation>
          <xsd:documentation>The grand total cost of all the purchases made.  The calculated cost should match the total payment amount (single or multiple forms of payment)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Payment" type="Payment.Misc" maxOccurs="2"/>
      <xsd:element name="DetailLevel" type="RequestedDetailLevel"/>
    </xsd:sequence>
    <xsd:attributeGroup ref="MISC_PayloadStdAttributes"/>
  </xsd:complexType>
  <xsd:complexType name="CollectMiscFeeRS">
    <xsd:sequence>
      <xsd:element name="Header" type="STL_Header.RS"/>
      <xsd:element name="AgentPOS" type="POS.Misc" minOccurs="0"/>
      <xsd:element name="Transaction" type="MiscTransInfo" minOccurs="0"/>
      <xsd:element name="Parameters" type="Collect.MiscTransInfo" minOccurs="0"/>
      <xsd:element name="Fees" type="FeesType" minOccurs="0" maxOccurs="99"/>
      <xsd:element name="TotalCost" type="Amount" minOccurs="0"/>
      <xsd:element name="Payment" type="Payment.Misc" minOccurs="0" maxOccurs="2"/>
    </xsd:sequence>
    <xsd:attributeGroup ref="MISC_PayloadStdAttributes"/>
  </xsd:complexType>
  <xsd:complexType name="CalculateExchangeCostRQ">
    <xsd:sequence>
      <xsd:element name="Header" type="STL_Header.RQ" minOccurs="0"/>
      <xsd:element name="AgentPOS" type="POS.Misc"/>
      <xsd:element name="Transaction" type="MiscTransInfo"/>
      <xsd:element name="Fees" type="Fees.Linked.Exchange"/>
      <xsd:element name="TotalCost" type="Amount">
        <xsd:annotation>
          <xsd:documentation>The grand total cost of all the purchases made.  The calculated cost should match the total payment amount (single or multiple forms of payment)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ExchangeDoc" type="ExchangeDocument.Misc"/>
      <xsd:element name="DetailLevel" type="RequestedDetailLevel"/>
    </xsd:sequence>
    <xsd:attributeGroup ref="MISC_PayloadStdAttributes"/>
  </xsd:complexType>
  <xsd:complexType name="CalculateExchangeCostRS">
    <xsd:sequence>
      <xsd:element name="Header" type="STL_Header.RS"/>
      <xsd:element name="AgentPOS" type="POS.Misc" minOccurs="0"/>
      <xsd:element name="Transaction" type="MiscTransInfo" minOccurs="0"/>
      <xsd:element name="Fees" type="Fees.Linked.Exchange" minOccurs="0" maxOccurs="99"/>
      <xsd:element name="TotalCost" type="Amount" minOccurs="0"/>
      <xsd:element name="ExchangeDoc" type="ExchangeDocument.Details" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="ExchangeCost" type="ExchangeCost.Misc" minOccurs="0"/>
    </xsd:sequence>
    <xsd:attributeGroup ref="MISC_PayloadStdAttributes"/>
  </xsd:complexType>
  <xsd:complexType name="Exchange.MiscTransInfo">
    <xsd:sequence>
      <xsd:element name="PrinterLniata" type="StringLength6" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>A printer line address where the document will be printed.  A printer should have been assigned by the agent (e.g., F20204)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PrinterStock" type="AlphaNumericStringLength2" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>A stock code used to assign a printer.  Mapped to:  SR05_11_PSTK</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PrintTaxCpn" type="xsd:boolean" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>An indicator  that a tax coupon will be printed.  Mapped to:  SR05_11_TCPN</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CarrierOverride" type="AlphaNumericStringLength2to3" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Validating carrier override for host partitions containing more than one carrier (ie LAN). Mapped to:
            SR05_11_OVR, SR05_11_OVRE</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AgencyIdOverride" type="EmptyOrNumericStringLength7" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>AGID - Agency ID override. An override for non-IATA "direct connect" Agency 7-digit numeric identifier or a blank value (meaning that the AGID element should not be added to the resulting EMD document). In case it’s not provided, the AGID element will be copied to the EMD from the PNR. Mapped to: SR05_D3_DATA with SR05_D3_TYPE = 'A'</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ExchangeDocumentforEMDRQ">
    <xsd:sequence>
      <xsd:element name="Header" type="STL_Header.RQ" minOccurs="0"/>
      <xsd:element name="AgentPOS" type="POS.Misc"/>
      <xsd:element name="Transaction" type="MiscTransInfo"/>
      <xsd:element name="Parameters" type="Exchange.MiscTransInfo" minOccurs="0"/>
      <xsd:element name="Fees" type="Fees.Linked"/>
      <xsd:element name="TotalCost" type="Amount">
        <xsd:annotation>
          <xsd:documentation>The grand total cost of all the purchases made.  The calculated cost should match the total payment amount (single or multiple forms of payment)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Payment" type="Payment.Misc" maxOccurs="2"/>
      <xsd:element name="ExchangeCost" type="ExchangeCost.Misc" minOccurs="0"/>
      <xsd:element name="DetailLevel" type="RequestedDetailLevel"/>
    </xsd:sequence>
    <xsd:attributeGroup ref="MISC_PayloadStdAttributes"/>
  </xsd:complexType>
  <xsd:complexType name="ExchangeDocumentforEMDRS">
    <xsd:sequence>
      <xsd:element name="Header" type="STL_Header.RS"/>
      <xsd:element name="AgentPOS" type="POS.Misc" minOccurs="0"/>
      <xsd:element name="Transaction" type="MiscTransInfo" minOccurs="0"/>
      <xsd:element name="Parameters" type="Exchange.MiscTransInfo" minOccurs="0"/>
      <xsd:element name="Fees" type="Fees.Linked" minOccurs="0" maxOccurs="99"/>
      <xsd:element name="TotalCost" type="Amount" minOccurs="0"/>
      <xsd:element name="Payment" type="Payment.Misc" minOccurs="0" maxOccurs="2"/>
      <xsd:element name="ExchangeDoc" type="ExchangeDocument.Details" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="ExchangeCost" type="ExchangeCost.Misc" minOccurs="0"/>
      <xsd:element name="RefundDoc" type="DocumentNumber" minOccurs="0"/>
    </xsd:sequence>
    <xsd:attributeGroup ref="MISC_PayloadStdAttributes"/>
  </xsd:complexType>
  <xsd:complexType name="CreditCard.Misc">
    <xsd:sequence>
      <xsd:element name="Code" type="PaymentCardCode">
        <xsd:annotation>
          <xsd:documentation>A card type used for paying a service or item (e.g, MC, VI, BT, BA, etc)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Number" type="Number.ClearCreditCard" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>An account number associated to card type
            Note: Only Number OR TokenizedNumber is allowed  in the request</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TokenizedNumber" type="Number.CreditCard" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Tokenized credit card number
            Note: Only Number OR TokenizedNumber is allowed  in the request</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ExpiryDate" type="MMYYDate">
        <xsd:annotation>
          <xsd:documentation>Credit card expiration date</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="SecurityCode" type="StringLength1to6" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Security card code of the payment method used (CSC)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ApprovalCode" type="AlphaNumericStringLength1to8" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>An approval code associated to payment.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ExtendedPayCode" type="AlphaNumericStringLength2" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>A code associated to the extended payment of the credit card used.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="EffectiveDate" type="MMYYDate" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>required if Maestro CC</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CompressedNumber" type="StringLength1to50" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>required if Maestro CC</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="transactionID" type="StringLength1to15"/>
  </xsd:complexType>
  <xsd:complexType name="Customer.Misc">
    <xsd:sequence>
      <xsd:element name="CustomerDetails" type="CustomerDetails.Misc" minOccurs="0"/>
      <xsd:element name="Message" type="CustomerMessage.Misc" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
    <xsd:attribute name="lastName" type="PNRName" use="required">
      <xsd:annotation>
        <xsd:documentation>The last name of the customer who purchased a ticket or service.  Mapped to: SR05_32_LNAM</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="firstName" type="PNRName" use="required">
      <xsd:annotation>
        <xsd:documentation>The first name of the customer who purchased the ticket or service.  Mapped to: SR05_32_FNAM</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="Customer.Misc.Override">
    <xsd:sequence>
      <xsd:element name="CustomerDetails" type="CustomerDetails.Misc" minOccurs="0"/>
      <xsd:element name="Override" type="Override.Misc" minOccurs="0"/>
    </xsd:sequence>
    <xsd:attribute name="lastName" type="PNRName" use="required">
      <xsd:annotation>
        <xsd:documentation>The last name of the customer who purchased a ticket or service.  Mapped to: SR05_32_LNAM</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="firstName" type="PNRName" use="required">
      <xsd:annotation>
        <xsd:documentation>The first name of the customer who purchased the ticket or service.  Mapped to: SR05_32_FNAM</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="CustomerDetails.Misc">
    <xsd:sequence>
      <xsd:element name="FrequentFlyer" type="FrequentFlyer.Misc" minOccurs="0"/>
      <xsd:element name="AgencyIataNumber" type="StringLength1to8" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>IATA Number of an agency for revenue and commission tracking. Mapped to: SR05_32_IATA</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="nameRefNumber" type="xsd:decimal">
      <xsd:annotation>
        <xsd:documentation>A name number associated to the traveller or purchaser in the PNR (e.g. 1.1)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="nameId" type="NumericLength1to2">
      <xsd:annotation>
        <xsd:documentation>A unique id that is associated to a passenger name in the PNR. Mapped to:  SR05_31_NID</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="pnrLocator" type="UpperCaseAlphaLength6">
      <xsd:annotation>
        <xsd:documentation>A code associated to the reservations of the customer purchasing an ancillary service.  Mapped to: SR05_31_PNR</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="CustomerMessage.Misc">
    <xsd:annotation>
      <xsd:documentation>Mapped to SR05_D3_DATA</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="StringLength1to100">
        <xsd:attribute name="number" type="xsd:int">
          <xsd:annotation>
            <xsd:documentation>Mapped to SR05_D3_PXID
              Put sequential number in per customer as each customer appears in XML for request.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="type">
          <xsd:annotation>
            <xsd:documentation>Mapped to SR05_D3_TYPE
              Example values:
              V = VCR Remarks</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:length value="1"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="DiagnosticResults">
    <xsd:sequence>
      <xsd:any namespace="##other" processContents="strict" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="DocumentNumber">
    <xsd:simpleContent>
      <xsd:extension base="StringLength13to15">
        <xsd:attribute name="couponNumber" type="AlphaNumericStringLength1to5"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="Emd.FreqFlyer">
    <xsd:sequence>
      <xsd:element name="Number" type="StringLength1to20"/>
      <xsd:element name="AirlineCode" type="AirlineCode"/>
      <xsd:element name="TierLevel" type="NumericStringLength1to5"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Emd.Service">
    <xsd:sequence>
      <xsd:element name="SubCode" type="UpperCaseAlphaNumericLength3"/>
      <xsd:element name="RFIC" type="AlphaNumericStringLength1">
        <xsd:annotation>
          <xsd:documentation>Reason For Issuance Code</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Group" type="ATPCO.Group"/>
      <xsd:element name="CommercialName" type="StringLength0to128"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="EndorsementType">
    <xsd:simpleContent>
      <xsd:extension base="StringLength1to60">
        <xsd:attribute name="type">
          <xsd:annotation>
            <xsd:documentation>Mapped to SR05_3A_TYPE</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:length value="4"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="ErrorMessage">
    <xsd:annotation>
      <xsd:documentation>Free text and code provided by the application or system that detected the error. Contants may be anything the system detecting the error chooses to convey. Used by service consumers.  Codes and/or messages should be agreed upon by service users. Do not use for structured data, use parameters instead.</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="Text.Long">
        <xsd:attribute name="code" type="xsd:integer"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="ExchangeDocument.Misc">
    <xsd:sequence>
      <xsd:element name="DocumentNumber" type="DocumentNumber"/>
      <xsd:element name="IssueDate" type="xsd:date" minOccurs="0"/>
      <xsd:element name="IssuePlace" type="AlphaNumericStringLength1to5" minOccurs="0"/>
      <xsd:element name="OrigFop" type="StringLength1to50" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ExchangeDocument.Details">
    <xsd:sequence>
      <xsd:element name="DocumentNumber" type="StringLength13to15">
        <xsd:annotation>
          <xsd:documentation>
            Mapped to T2 database:
            TKT_DOC.TKT_DOC_ID
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="IssueDate" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            Mapped to T2 database: TKT_DOC.TKT_DOC_LCL_ISSUE_DTM
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DocumentType" type="StringLength1to3" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            Mapped to T2 database: TKT_DOC.TKT_DOC_TYP_CD
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="IssuePlace" type="AlphaNumericStringLength1to5" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            Mapped to T2 database: TKT_TNA.ISSUING_AGT_WRK_LOC_ID
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Reservation" type="UpperCaseAlphaLength6" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            Mapped to T2 database: TKT_DOC.SAB_RES_ID
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="FirstName" type="ProperName" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            Mapped to T2 database: TKT_DOC_CST.CST_FRST_NM;
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="LastName" type="ProperName" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            Mapped to T2 database: TKT_DOC.CST_LAST_NM;
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TotalAmount" type="Amount" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The exchange value of the document.  </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Payment" type="StringLength1to50" minOccurs="0" maxOccurs="2"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ExchangeCost.Misc">
    <xsd:sequence>
      <xsd:element name="Exchange" type="Amount.Basic" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Exchange Amount</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="result" type="AlphaNumericStringLength2" use="required">
      <xsd:annotation>
        <xsd:documentation>Code associated to the result of the exchange cost comparison.  Values can be RF - refund, AC - add/collect, EE - even exchange</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="refundFOP" type="AlphaNumericStringLength2to3">
      <xsd:annotation>
        <xsd:documentation>Refund FOP. Available only when result attribute is set to RF</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="Fee.Grouped">
    <xsd:sequence>
      <xsd:element name="FeeDetails" type="FeeDetails.Misc"/>
      <xsd:element name="OptionalService" type="OptionalService" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Specific information about optional service (OC) as published in ATPCO or Merchandiziing</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="FrequentFlyer" minOccurs="0"/>
      <xsd:element name="Valuation" type="Valuation" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Additional charges associated to a high value or excess valuation of an item</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Tax" type="Tax.Misc" minOccurs="0" maxOccurs="99"/>
      <xsd:element name="AirportTax" type="AirportTax" minOccurs="0" maxOccurs="32"/>
      <xsd:element name="IssuedDocumentNumber" type="DocumentNumber" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Fee.Linked">
    <xsd:sequence>
      <xsd:element name="FeeDetails" type="FeeDetails.Misc"/>
      <xsd:element name="FOPDetails" type="FOPDetails.Misc" minOccurs="0"/>
      <xsd:element name="OptionalService" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Specific information about optional service (OC) as published in ATPCO or Merchandiziing</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:complexContent>
            <xsd:extension base="OptionalService"/>
          </xsd:complexContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="FrequentFlyer" type="Emd.FreqFlyer" minOccurs="0"/>
      <xsd:element name="Valuation" type="Valuation" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Additional charges associated to a high value or excess valuation of an item</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AssociatedFlight" type="Flight.Misc" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>Fee can be associated to a flight coupon.  When applicable, connecting or downline flights are included in this element.  Use AssociatedFlight if miscellaneous items have PRAS codes that required flight coupons and are originally processed thru the following MISC options:

            MISC2/4 - CHANGE FEE
            MISC2/5 - OPTIONAL FEE
            EMD-A (Future)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Tax" type="Tax.Misc" minOccurs="0" maxOccurs="99"/>
      <xsd:element name="AirportTax" type="AirportTax" minOccurs="0" maxOccurs="32"/>
      <xsd:element name="AssociatedTicketNumber" type="DocumentNumber" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Fee can be associated to a ticket number</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="IssuedDocumentNumber" type="DocumentNumber" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Fee.Linked.Exchange">
    <xsd:sequence>
      <xsd:element name="FeeDetails" type="FeeDetails.Misc"/>
      <xsd:element name="OptionalService" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Specific information about optional service (OC) as published in ATPCO or Merchandiziing</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:complexContent>
            <xsd:extension base="OptionalService"/>
          </xsd:complexContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="AssociatedFlight" type="Flight.Misc" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>Fee can be associated to a flight coupon.  When applicable, connecting or downline flights are included in this element.  Use AssociatedFlight if miscellaneous items have PRAS codes that required flight coupons and are originally processed thru the following MISC options:

            MISC2/4 - CHANGE FEE
            MISC2/5 - OPTIONAL FEE
            EMD-A (Future)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Tax" type="Tax.Misc" minOccurs="0" maxOccurs="99"/>
      <xsd:element name="AssociatedTicketNumber" type="DocumentNumber" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Fee can be associated to a ticket number</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="FeeDetails.Misc">
    <xsd:sequence>
      <xsd:element name="Base" type="Amount" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The base fee amount associated to the item or service which does not include taxes.   Mapped to: SR05_C1_BAMT</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Equiv" type="Amount" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The equivalent or converted fee amount associated to the item or service which does not include taxes.   Mapped to: SR05_C1_BAMT</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TotalTax" type="Amount" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The total fee taxes associated to the item or service purchased.   Mapped to: SR05_C1_TAMT</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Total" type="Amount" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The total fee amount associated to the service including taxes.   Mapped to: SR05_C1_FAMT</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="EffectiveDate" type="xsd:date" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The date the fee and amounts will be effective or can be used for purchase.  Mapped to:  SR05_C1_EFFD</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="code" type="StringLength1to3" use="required">
      <xsd:annotation>
        <xsd:documentation>PRAS code associated to the  service or item.  Mapped to: SR05_C1_PRAS</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="description" type="StringLength1to21">
      <xsd:annotation>
        <xsd:documentation>Description of the purchased item or service.  Mapped to: SR05_C1_DESC</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="quantity" type="NumericLength1to2">
      <xsd:annotation>
        <xsd:documentation>The number of items purchased per PRAS code.  Mapped to: SR05_C1_IQTY</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="FOPDetails.Misc">
    <xsd:sequence>
      <xsd:element name="FOP" type="FOP.Detail.Misc" minOccurs="1" maxOccurs="2">
        <xsd:annotation>
          <xsd:documentation>Amount taken from particular form of payment to fulfill partially or fully the given service fee. Mapped to ??SR05_??</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="FOP.Detail.Misc">
    <xsd:simpleContent>
      <xsd:extension base="_amount">
        <xsd:attribute name="paymentRefNumber" use="required">
          <xsd:annotation>
            <xsd:documentation>Reference to the form of payment (//Payment/@refNumber) being used to pay for given service fee (fully or partially).  Mapped to: ??SR05_??</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:integer">
              <xsd:minInclusive value="1"/>
              <xsd:maxInclusive value="2"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="FeeItem.Misc">
    <xsd:sequence>
      <xsd:element name="FeeDetails" type="FeeDetails.Misc"/>
      <xsd:element name="Tax" type="Tax.Misc" minOccurs="0" maxOccurs="99"/>
      <xsd:element name="Flight" type="Flight.Misc" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="Usage" type="FeeUsageParameters" minOccurs="0"/>
      <xsd:element name="OptionalService" type="OptionalService.GetMiscFees" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="FeeList.Misc">
    <xsd:sequence>
      <xsd:element name="Fee" type="FeeItem.Misc" maxOccurs="999"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="FeeParameterMaximum">
    <xsd:attribute name="ticket" type="xsd:int">
      <xsd:annotation>
        <xsd:documentation>Maximum number oif tickets which may be associated to PRAS item</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="segment" type="xsd:int">
      <xsd:annotation>
        <xsd:documentation>Maximum number of flight segments which may be associated to PRAS item</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="Fees.Grouped">
    <xsd:sequence>
      <xsd:element name="Customer" type="Customer.Misc"/>
      <xsd:element name="TicketNumber" type="DocumentNumber" minOccurs="0" maxOccurs="6">
        <xsd:annotation>
          <xsd:documentation>One or more fees are related to one or more ticket numbers. Use TicketNumber if miscellaneous items have PRAS codes that are originally processed thru the following MISC options and required ticket numbers:

            MISC2/1 - SALES ITEMS
            MISC2/2 - SERVICES
            MISC2/3 - UNPAID TAXES
            MISC3 - EXCESS BAGGAGE TICKET</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Flight" type="Flight.Misc" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>One or more fees are related to one or more flight coupons.  Use Flight if miscellaneous items have PRAS codes that are originally processed thru the following MISC options and required flight coupons:

            MISC2/1 - SALES ITEMS
            MISC2/2 - SERVICES
            MISC2/3 - UNPAID TAXES
            MISC3 - EXCESS BAGGAGE TICKET</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Fee" type="Fee.Grouped" maxOccurs="99"/>
      <xsd:element name="Endorsement" type="EndorsementType" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>Mapped to SR05_3A_ENDO</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Fees.Grouped.Override">
    <xsd:sequence>
      <xsd:element name="Customer" type="Customer.Misc.Override"/>
      <xsd:element name="TicketNumber" type="DocumentNumber" minOccurs="0" maxOccurs="6">
        <xsd:annotation>
          <xsd:documentation>One or more fees are related to one or more ticket numbers. Use TicketNumber if miscellaneous items have PRAS codes that are originally processed thru the following MISC options and required ticket numbers:

            MISC2/1 - SALES ITEMS
            MISC2/2 - SERVICES
            MISC2/3 - UNPAID TAXES
            MISC3 - EXCESS BAGGAGE TICKET</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Flight" type="Flight.Misc" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>One or more fees are related to one or more flight coupons.  Use Flight if miscellaneous items have PRAS codes that are originally processed thru the following MISC options and required flight coupons:

            MISC2/1 - SALES ITEMS
            MISC2/2 - SERVICES
            MISC2/3 - UNPAID TAXES
            MISC3 - EXCESS BAGGAGE TICKET</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Fee" type="Fee.Grouped" maxOccurs="99"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Fees.Linked">
    <xsd:sequence>
      <xsd:element name="Customer" type="Customer.Misc"/>
      <xsd:element name="Fee" type="Fee.Linked" maxOccurs="99"/>
      <xsd:element name="Endorsement" type="EndorsementType" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>Mapped to SR05_3A_ENDO</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Fees.Linked.Exchange">
    <xsd:sequence>
      <xsd:element name="Customer" type="Customer.Misc"/>
      <xsd:element name="Fee" type="Fee.Linked.Exchange" maxOccurs="99"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Fees.Linked.Override">
    <xsd:sequence>
      <xsd:element name="Customer" type="Customer.Misc.Override"/>
      <xsd:element name="Fee" type="Fee.Linked" maxOccurs="99"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="FeesType">
    <xsd:group ref="Fees.Association" minOccurs="0"/>
  </xsd:complexType>
  <xsd:complexType name="FeesType.Override">
    <xsd:group ref="Fees.Association.Override" minOccurs="0"/>
  </xsd:complexType>
  <xsd:complexType name="FeeUsageParameters">
    <xsd:sequence>
      <xsd:element name="Max" type="FeeParameterMaximum" minOccurs="0"/>
      <xsd:element name="Suffix" type="StringLength1to3" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>PRAS item suffix</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Group" type="NumericLength1to2" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Group number associated to PRAS items. Multiple PRAS items with the same group number print together on MSR where possible.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="ticket">
      <xsd:annotation>
        <xsd:documentation>Related ticket:
          Y = Required
          N = Not Required
          A = Association Required
          Mapped to SR05_CA_ITKT</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="Y"/>
          <xsd:enumeration value="N"/>
          <xsd:enumeration value="A"/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="PNR">
      <xsd:annotation>
        <xsd:documentation>Related PNR:
          Y = Required
          N = Not Required
          Mapped to SR05_CA_IPNR</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="Y"/>
          <xsd:enumeration value="N"/>
          <xsd:enumeration value="A"/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="segment">
      <xsd:annotation>
        <xsd:documentation>Related flight segment:
          Y = Required
          N = Not Required
          A = Association Required
          Mapped to SR05_CA_ISEG</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="Y"/>
          <xsd:enumeration value="N"/>
          <xsd:enumeration value="A"/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="override">
      <xsd:annotation>
        <xsd:documentation>Overridable indicator:
          Y = Overridable
          N = Not Overridable
          Mapped to SR05_CA_IOVR</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="Y"/>
          <xsd:enumeration value="N"/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="null">
      <xsd:annotation>
        <xsd:documentation>Null indicator:
          Y = Null allowed
          N = Null not allowed
          Mapped to SR05_CA_INUL</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="Y"/>
          <xsd:enumeration value="N"/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="coupon">
      <xsd:annotation>
        <xsd:documentation>Related coupon:
          Y = Required
          N = Not Required
          A = Association Required
          Mapped to SR05_CA_ICPN</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="Y"/>
          <xsd:enumeration value="N"/>
          <xsd:enumeration value="A"/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="fakePras">
      <xsd:annotation>
        <xsd:documentation>Fake PRAS code
          Y = Fake PRAS code
          N = Real PRAS code
          Mapped to SR05_CA_INRL</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="Y"/>
          <xsd:enumeration value="N"/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="Flight.Misc">
    <xsd:sequence>
      <xsd:element name="CarrierCode" type="AirlineCode">
        <xsd:annotation>
          <xsd:documentation>The carrier code of the flight associated to the service.  Mapped to:  SR05_42_CXRC, SR05_C4_CXRC</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="OperatingCarrierCode" type="AirlineCode" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The operating carrier code of the flight associated to the service.  Mapped to:  SR05_C4_OPRC</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="FlightNumber" type="FlightNumber">
        <xsd:annotation>
          <xsd:documentation>The flight number associated to the service.   Mapped to: SR05_42_FLTN, SR05_C4_FLTN</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ClassOfService" type="CabinType.3Chars">
        <xsd:annotation>
          <xsd:documentation>The flight class of service associated to the ancillary service.  Mapped to: SR05_42_CLSV, SR05_C4_CLSV</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DepartureCity" type="CityCode">
        <xsd:annotation>
          <xsd:documentation>The origin or depature city code of the flight associated to the service.   Mapped to: SR05_42_DCTY, SR05_C4_DCTY</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ArrivalCity" type="CityCode">
        <xsd:annotation>
          <xsd:documentation>The destination or arrival city of the flight associated to the service.   Mapped to: SR05_42_ACTY, SR05_C4_ACTY</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DepartureDate" type="xsd:date">
        <xsd:annotation>
          <xsd:documentation>The departure date of the flight associated to the service.  Mapped to: SR05_42_CDTE, SR05_C4_CDTE</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="StopOver" type="AlphaNumericStringLength1" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>A code that applies to whether the flight is a stop over or not (X, O).   Mapped to: SR05_42_STOV, SR05_C4_STOV</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="FrequentFlyer" type="Emd.FreqFlyer" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Frequent Flyer information. Mapped to: SR05_C4_FFN, SR05_C4_FAL, SR05_C4_FLV</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AssociatedTicketNumber" type="DocumentNumber" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Associated ticket number for segment. Mapped to: SR05_C4_TKT, SR05_C4_CPN</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="IssuedDocumentNumber" type="DocumentNumber" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Generated ticket number for segment. Mapped to: SR05_C4_GTKT, SR05_C4_GCPN</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="segmentID" type="xsd:int">
      <xsd:annotation>
        <xsd:documentation>Segment ID in the PNR. Will be provided by orchestrated web service. Not expected of agent via Interact or other interface.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="segmentNumber" type="xsd:int">
      <xsd:annotation>
        <xsd:documentation>Segment number as shown in the PNR.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="Flight.Misc.Exchange">
    <xsd:sequence>
      <xsd:element name="CarrierCode" type="AirlineCode">
        <xsd:annotation>
          <xsd:documentation>The carrier code of the flight associated to the service.  Mapped to:  SR05_42_CXRC, SR05_C4_CXRC</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="FlightNumber" type="FlightNumber">
        <xsd:annotation>
          <xsd:documentation>The flight number associated to the service.   Mapped to: SR05_42_FLTN, SR05_C4_FLTN</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ClassOfService" type="CabinType.3Chars">
        <xsd:annotation>
          <xsd:documentation>The flight class of service associated to the ancillary service.  Mapped to: SR05_42_CLSV, SR05_C4_CLSV</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DepartureCity" type="CityCode">
        <xsd:annotation>
          <xsd:documentation>The origin or depature city code of the flight associated to the service.   Mapped to: SR05_42_DCTY, SR05_C4_DCTY</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ArrivalCity" type="CityCode">
        <xsd:annotation>
          <xsd:documentation>The destination or arrival city of the flight associated to the service.   Mapped to: SR05_42_ACTY, SR05_C4_ACTY</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DepartureDate" type="xsd:date">
        <xsd:annotation>
          <xsd:documentation>The departure date of the flight associated to the service.  Mapped to: SR05_42_CDTE, SR05_C4_CDTE</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="StopOver" type="AlphaNumericStringLength1" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>A code that applies to whether the flight is a stop over or not (X, O).   Mapped to: SR05_42_STOV, SR05_C4_STOV</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AssociatedTicketNumber" type="DocumentNumber" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Associated ticket number for segment. Mapped to: SR05_C4_TKT, SR05_C4_CPN</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="segmentNumber" type="xsd:int">
      <xsd:annotation>
        <xsd:documentation>Segment number as shown in the PNR.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="FormOfIdentification.Misc">
    <xsd:sequence>
      <xsd:element name="Number" type="AlphaNumericStringLength1to25">
        <xsd:annotation>
          <xsd:documentation>A number that would identify the person who purchases the service.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="code" use="required">
      <xsd:annotation>
        <xsd:documentation>Unique code that identifies the typ of id the person has used for purchasing the service or item</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="PrimaryIdCodeList"/>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="secondaryId">
      <xsd:annotation>
        <xsd:documentation>Unique code associiated to the secondary ID</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="SecondaryIdCodeList"/>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="state" type="AlphaLength2"/>
  </xsd:complexType>
  <xsd:complexType name="FormOfPaymentChoice.Misc">
    <xsd:choice>
      <xsd:element name="CreditCard" type="CreditCard.Misc">
        <xsd:annotation>
          <xsd:documentation>FOP Code=CC</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ExchangeDoc" type="ExchangeDocument.Misc">
        <xsd:annotation>
          <xsd:documentation>FOP Code=ET</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Cash" type="StringLength0to128">
        <xsd:annotation>
          <xsd:documentation>Presensence of this element indicates a cash transaction; FOP Code=CA</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Check" type="Check.Misc">
        <xsd:annotation>
          <xsd:documentation>FOP Code=CK</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Invoice" type="Invoice.Misc">
        <xsd:annotation>
          <xsd:documentation>FOP Code=IN</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="GovTransRequest" type="GTR.Misc">
        <xsd:annotation>
          <xsd:documentation>FOP Code=GR</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PrepaidTicketAdvice" type="PTA.Misc">
        <xsd:annotation>
          <xsd:documentation>FOP Code = PT</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Other" type="OtherFOP.Misc"/>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="FrequentFlyer.Misc">
    <xsd:attribute name="number" type="StringLength1to20" use="required">
      <xsd:annotation>
        <xsd:documentation>Same as customer loyalty number.  Mapped to: SR05_31_FFN</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="airlineCode" type="AirlineCode" use="required">
      <xsd:annotation>
        <xsd:documentation>Airline code of the customer loyalty number.   Mapped to: SR05_31_FAL</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="GTR.Misc">
    <xsd:sequence>
      <xsd:element name="Number" type="StringLength1to13"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="GetMiscFeesRQ">
