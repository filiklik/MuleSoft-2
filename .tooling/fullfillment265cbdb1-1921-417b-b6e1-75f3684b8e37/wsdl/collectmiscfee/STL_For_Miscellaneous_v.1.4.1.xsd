      <xsd:pattern value="[A-Z0-9]{1,3}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="UpperCaseAlphaNumericLength3">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[A-Z0-9]{3}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="WeightCode">
    <xsd:annotation>
      <xsd:documentation>codes:L=Pounds, K=Kilos, P=Pieces, N=Nil</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="L"/>
      <xsd:enumeration value="K"/>
      <xsd:enumeration value="P"/>
      <xsd:enumeration value="N"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="_PaymentCardType">
    <xsd:restriction base="UpperCaseAlphaLength1to2">
      <xsd:enumeration value="AX">
        <xsd:annotation>
          <xsd:documentation>American Express</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BC">
        <xsd:annotation>
          <xsd:documentation>Bank Card</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BL">
        <xsd:annotation>
          <xsd:documentation>Carte Bleu</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="CB">
        <xsd:annotation>
          <xsd:documentation>Carte Blanche</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DN">
        <xsd:annotation>
          <xsd:documentation>Diners Club</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DS">
        <xsd:annotation>
          <xsd:documentation>Discover Card</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="EC">
        <xsd:annotation>
          <xsd:documentation>Eurocard</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="JC">
        <xsd:annotation>
          <xsd:documentation>Japanese Credit Bureau Credit Card</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="MA">
        <xsd:annotation>
          <xsd:documentation>Maestro</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="MC">
        <xsd:annotation>
          <xsd:documentation>Master Card</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="TP">
        <xsd:annotation>
          <xsd:documentation>Universal Air Travel Card</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="VI">
        <xsd:annotation>
          <xsd:documentation>Visa</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="_PaymentCardType_Other">
    <xsd:restriction base="UpperCaseAlphaLength1to2"/>
  </xsd:simpleType>
  <xsd:simpleType name="_amount">
    <xsd:restriction base="xsd:decimal">
      <xsd:fractionDigits value="3"/>
      <xsd:maxInclusive value="100000000"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="_flightNumberLiterals">
    <xsd:annotation>
      <xsd:documentation>This private type enumerates flight number literals used for ground links and open segments in an itinerary.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="OPEN">
        <xsd:annotation>
          <xsd:documentation>Used in lieu of a flight number when a specific flight number is unknown but service is present.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ARNK">
        <xsd:annotation>
          <xsd:documentation>Used in lieu of a flight number when surface transportation is used when there is a break in the continuity of the flight itinerary.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="_flightNumberPattern">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9]{1,4}[A-Z]?"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:group name="Fees.Association">
    <xsd:annotation>
      <xsd:documentation>Associating Fees</xsd:documentation>
    </xsd:annotation>
    <xsd:choice>
      <xsd:element name="Linked" type="Fees.Linked"/>
      <xsd:element name="Grouped" type="Fees.Grouped"/>
    </xsd:choice>
  </xsd:group>
  <xsd:group name="Fees.Association.Override">
    <xsd:annotation>
      <xsd:documentation>Associating Fees</xsd:documentation>
    </xsd:annotation>
    <xsd:choice>
      <xsd:element name="Linked" type="Fees.Linked.Override"/>
      <xsd:element name="Grouped" type="Fees.Grouped.Override"/>
    </xsd:choice>
  </xsd:group>
  <xsd:attributeGroup name="MISC_PayloadStdAttributes">
    <xsd:attribute name="version" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Versioned message value</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="primaryLangID" type="xsd:language"/>
    <xsd:attribute name="altLangID" type="xsd:language"/>
  </xsd:attributeGroup>
  <xsd:attributeGroup name="commonOptionalServiceAttributes">
    <xsd:attribute name="RFIC" type="AlphaNumericStringLength1" use="optional">
      <xsd:annotation>
        <xsd:documentation>Reason for issuance code. Mapped to: SR05_C5_RFIC or C1_RFIC for GetMiscFees response</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="subCode" type="StringLength1to3" use="optional">
      <xsd:annotation>
        <xsd:documentation>Reason for issuance sub-code. Mapped to: SR05_C5_RFISC or C1_SRFI for GetMiscFees</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="ssrCode" type="AlphaNumericStringLength4" use="optional">
      <xsd:annotation>
        <xsd:documentation>Service code in the PNR associated to the item. Mapped to SR05_C5_SSR or C1_SCOD for GetMiscFees response</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:attributeGroup>
</xsd:schema>